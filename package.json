{
  "name": "n8n-nodes-fishaudio",
  "version": "0.1.0",
  "description": "The official n8n node for the Fish Audio API.",
  "license": "MIT",
  "homepage": "https://fish.audio",
  "keywords": [
    "n8n-community-node-package",
    "fish audio",
    "fishaudio",
    "text to speech",
    "tts",
    "voice cloning",
    "voice synthesis",
    "ai voice",
    "speech synthesis",
    "voice generation",
    "n8n"
  ],
  "author": { "name": "James Ding", "email": "james@fish.audio" },
  "repository": {
    "type": "git",
    "url": "https://github.com/fishaudio/fish-audio-n8n.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": ["dist"],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": ["dist/credentials/FishaudioApi.credentials.js"],
    "nodes": ["dist/nodes/Fishaudio/Fishaudio.node.js"]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": { "n8n-workflow": "*" }
}
